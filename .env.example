# Development Environment
PROJECT_NAME=FastAPI Dev App
ENVIRONMENT=development
LOG_LEVEL=DEBUG

# PostgreSQL settings for development
POSTGRES_SERVER=localhost
POSTGRES_USER=postgres
POSTGRES_PASSWORD=your_postgres_password
POSTGRES_DB=fastapi_db
POSTGRES_PORT=15432

# Development server settings
HOST=0.0.0.0
PORT=8010

# Security (use simple values for development)
SECRET_KEY=dev-secret-key-change-this-in-production
ACCESS_TOKEN_EXPIRE_MINUTES=1440

# CORS for development (using default values from config.py)
# BACKEND_CORS_ORIGINS=http://localhost:3000,http://localhost:8080,http://localhost:5173

# Admin for development
FIRST_SUPERUSER_EMAIL=admin@example.com
FIRST_SUPERUSER_PASSWORD=admin123

# Redis settings for task queue and caching
REDIS_HOST=localhost
REDIS_PORT=6379
REDIS_PASSWORD=
REDIS_DB=0
REDIS_URL=redis://localhost:6379/0

# ARQ Task Queue Configuration
ARQ_MAX_JOBS=10
ARQ_JOB_TIMEOUT=300
ARQ_KEEP_RESULT=3600